@prefix : <http://www.valbormida.it/attilio/f1.owl> .
@prefix f1: <http://www.valbormida.it/attilio/f1.owl> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.valbormida.it/attilio/f1.owl> .

<http://www.valbormida.it/attilio/f1.owl> rdf:type owl:Ontology ;
                                           <http://purl.org/dc/elements/1.1/creator> "Attilio Sangalli"@it ;
                                           <http://purl.org/dc/elements/1.1/date> "2023-09"@it ;
                                           <http://purl.org/dc/elements/1.1/description> "Ontologia che illustra il mondo della Formula 1 nelle sue componenti: eventi, circuiti, scuderie e piloti partecipanti"@it ;
                                           <http://purl.org/dc/elements/1.1/title> "Formula1Ontology"@en ;
                                           :vann:preferredNamespacePrefix "F1"@it ;
                                           :vann:preferredNamespaceUri "http://www.semanticweb.org/attilio/ontologies/2022/9/f1#"@it ;
                                           rdfs:label "Formula1"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://www.valbormida.it/attilio/f1.owl#vann:preferredNamespacePrefix
:vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://www.valbormida.it/attilio/f1.owl#vann:preferredNamespaceUri
:vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.valbormida.it/attilio/f1.owl#acquistaMotore
:acquistaMotore rdf:type owl:ObjectProperty ;
                owl:inverseOf :fornisceMotore ;
                rdfs:domain :ScuderiaCliente ;
                rdfs:range :ScuderiaCostruttore ;
                rdfs:comment "mette in relazione una scuderia che non si costruisce la propria power unit con la scuderia che gliela fornisce"@it ;
                rdfs:label "acquistaMotore"@it .


###  http://www.valbormida.it/attilio/f1.owl#avvieneDurante
:avvieneDurante rdf:type owl:ObjectProperty ;
                owl:inverseOf :faSvolgere ;
                rdfs:domain :Gara ;
                rdfs:range :Evento ;
                rdfs:comment "mette in relazione la gara, in senso sportivo, con l'evento durante il quale tale gara si svolge"@it ;
                rdfs:label "avvieneDurante"@it .


###  http://www.valbormida.it/attilio/f1.owl#campioneCostruttori
:campioneCostruttori rdf:type owl:ObjectProperty ;
                     owl:inverseOf :haCampioneCostruttori ;
                     rdfs:domain :Scuderia ;
                     rdfs:range :Campionato ;
                     rdfs:comment "mette in relazione la squadra vincitrice del titolo costruttori con tale campionato"@it ;
                     rdfs:label "campioneCostruttori"@it .


###  http://www.valbormida.it/attilio/f1.owl#campioneMondiale
:campioneMondiale rdf:type owl:ObjectProperty ;
                  owl:inverseOf :haCampione ;
                  rdfs:domain :Pilota ;
                  rdfs:range :Campionato ;
                  rdfs:comment "mette in relazione il pilota che vince il titolo, con il relativo campionato"@it ;
                  rdfs:label "campioneMondiale"@it .


###  http://www.valbormida.it/attilio/f1.owl#compagnoDiTeam
:compagnoDiTeam rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:domain :Pilota ;
                rdfs:range :Pilota ;
                rdfs:comment "mette in relazione due piloti che corrono per la stessa scuderia"@it ;
                rdfs:label "compagnodiTeam"@it .


###  http://www.valbormida.it/attilio/f1.owl#correPer
:correPer rdf:type owl:ObjectProperty ;
          owl:inverseOf :haPilota ;
          rdfs:domain :Pilota ;
          rdfs:range :Scuderia ;
          rdfs:comment "mette in relazione un pilota con la scuderia per cui corre"@it ;
          rdfs:label "correPer"@it .


###  http://www.valbormida.it/attilio/f1.owl#detieneTrackRecord
:detieneTrackRecord rdf:type owl:ObjectProperty ;
                    rdfs:domain :Pilota ;
                    rdfs:range :Circuito ;
                    rdfs:comment "mette in relazione un pilota con il circuito in cui tale pilota detiene il record del tracciato"@it ;
                    rdfs:label "detieneTrackRecord"@it .


###  http://www.valbormida.it/attilio/f1.owl#edizioneDi
:edizioneDi rdf:type owl:ObjectProperty ;
            owl:inverseOf :haEdizione ;
            rdfs:domain :Evento ;
            rdfs:range :GranPremio ;
            rdfs:comment "mette in relazione l'edizione di un gran premio con la manifestazione di cui ne è un'edizione"@it ;
            rdfs:label "edizioneDi"@it .


###  http://www.valbormida.it/attilio/f1.owl#eventoDi
:eventoDi rdf:type owl:ObjectProperty ;
          owl:inverseOf :haEvento ;
          rdfs:domain :Evento ;
          rdfs:range :Campionato ;
          rdfs:comment "mette in relazione l'edizione di un gran premio con il campionato in cui tale edizione è svolta"@it ;
          rdfs:label "eventoDi"@it .


###  http://www.valbormida.it/attilio/f1.owl#faSvolgere
:faSvolgere rdf:type owl:ObjectProperty ;
            rdfs:comment "mette in relazione l'edizione di un gran premio con la gara sportiva che durante tale evento viene corsa"@it ;
            rdfs:label "faSvolgere"@it .


###  http://www.valbormida.it/attilio/f1.owl#fornisceMotore
:fornisceMotore rdf:type owl:ObjectProperty ;
                rdfs:domain :ScuderiaCostruttore ;
                rdfs:range :ScuderiaCliente ;
                rdfs:comment "mette in relazione una scuderia che costruisce la propria power unit con la scuderia a cui tale power unit è fornita"@it ;
                rdfs:label "fornisceMotore"@it .


###  http://www.valbormida.it/attilio/f1.owl#gpv
:gpv rdf:type owl:ObjectProperty ;
     owl:inverseOf :ottieneGPV ;
     rdfs:domain :Gara ;
     rdfs:range :Pilota ;
     rdfs:comment "mette in relazione una gara con il pilota che fa segnare il giro più veloce durante tale gara"@it ;
     rdfs:label "giroPiùVeloce"@it .


###  http://www.valbormida.it/attilio/f1.owl#haCampione
:haCampione rdf:type owl:ObjectProperty ;
            rdfs:domain :Campionato ;
            rdfs:range :Pilota ;
            rdfs:comment "mette in relazione un campionato con il pilota che vince il titolo piloti in palio per quel campionato"@it ;
            rdfs:label "haCampione"@it .


###  http://www.valbormida.it/attilio/f1.owl#haCampioneCostruttori
:haCampioneCostruttori rdf:type owl:ObjectProperty ;
                       rdfs:domain :Campionato ;
                       rdfs:range :Scuderia ;
                       rdfs:comment "mette in relazione un campionato con la scuderia che vince il titolo costruttori in palio per quel campionato"@it ;
                       rdfs:label "haCampioneCostruttori"@it .


###  http://www.valbormida.it/attilio/f1.owl#haEdizione
:haEdizione rdf:type owl:ObjectProperty ;
            rdfs:domain :GranPremio ;
            rdfs:range :Evento ;
            rdfs:comment "mette in relazione un gran premio con una sua edizione"@it ;
            rdfs:label "haEdizione"@it .


###  http://www.valbormida.it/attilio/f1.owl#haEvento
:haEvento rdf:type owl:ObjectProperty ;
          rdfs:domain :Campionato ;
          rdfs:range :Evento ;
          rdfs:comment "mette in relazione un campionato con un'edizione di gran premio che avviene in tale campionato"@it ;
          rdfs:label "haEvento"@it .


###  http://www.valbormida.it/attilio/f1.owl#haLocation
:haLocation rdf:type owl:ObjectProperty ;
            owl:inverseOf :haOspitato ;
            rdfs:domain :CircuitoOmologato ;
            rdfs:range :Evento ;
            owl:propertyChainAxiom ( :faSvolgere
                                     :tenutaPresso
                                   ) ;
            rdfs:comment "proprietà che lega un'edizione di Gran Premio al circuito su cui si è svolta la relativa gara"@it ;
            rdfs:label "haLocation"@it .


###  http://www.valbormida.it/attilio/f1.owl#haOspitato
:haOspitato rdf:type owl:ObjectProperty ;
            rdfs:comment "proprietà che lega un circuito alle edizioni di un Gran Premio la cui gara si è tenuta su tale circuito"@it ;
            rdfs:label "haOspitato"@it .


###  http://www.valbormida.it/attilio/f1.owl#haPartecipante
:haPartecipante rdf:type owl:ObjectProperty ;
                owl:inverseOf :partecipa ;
                rdfs:domain :Campionato ;
                rdfs:range :Scuderia ;
                rdfs:comment "mette in relazione un campionato con una scuderia iscritta a tale campionato"@it ;
                rdfs:label "haPartecipante"@it .


###  http://www.valbormida.it/attilio/f1.owl#haPilota
:haPilota rdf:type owl:ObjectProperty ;
          rdfs:domain :Scuderia ;
          rdfs:range :Pilota ;
          rdfs:comment "mette in relazione una scuderia con un pilota che corre per tale scuderia"@it ;
          rdfs:label "haPilota"@it .


###  http://www.valbormida.it/attilio/f1.owl#haPoleSitter
:haPoleSitter rdf:type owl:ObjectProperty ;
              owl:inverseOf :haPresoPole ;
              rdfs:domain :Gara ;
              rdfs:range :Pilota ;
              rdfs:comment "mette in relazione una gara con il pilota che ha ottenuto di partire dalla pole position in tale gara"@it ;
              rdfs:label "haPoleSitter"@it .


###  http://www.valbormida.it/attilio/f1.owl#haPresoPole
:haPresoPole rdf:type owl:ObjectProperty ;
             rdfs:domain :Pilota ;
             rdfs:range :Gara ;
             rdfs:comment "mette in relazione un pilota con una gara in cui tale pilota ha ottenuto di partire dalla pole position"@it ;
             rdfs:label "haPresoPole"@it .


###  http://www.valbormida.it/attilio/f1.owl#haPrimoEvento
:haPrimoEvento rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :haEvento ;
               rdfs:domain :Campionato ;
               rdfs:range :Evento ;
               rdfs:comment "mette in relazione un campionato con l'evento iniziale di tale campionato"@it ;
               rdfs:label "haPrimoEvento"@it .


###  http://www.valbormida.it/attilio/f1.owl#haScuderiaVincitore
:haScuderiaVincitore rdf:type owl:ObjectProperty ;
                     rdfs:domain :Gara ;
                     rdfs:range :Scuderia ;
                     rdfs:comment "mette in relazione una gara con la scuderia per cui corre il pilota che ha vinto tale gara"@it ;
                     rdfs:label "haScuderiaVincitrice"@it .


###  http://www.valbormida.it/attilio/f1.owl#haUltimoEvento
:haUltimoEvento rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :haEvento ;
                rdfs:domain :Campionato ;
                rdfs:range :Evento ;
                rdfs:comment "mette in relazione un campionato con l'ultimo evento che si tiene in tale campionato"@it ;
                rdfs:label "haUltimoEvento"@it .


###  http://www.valbormida.it/attilio/f1.owl#haVincitore
:haVincitore rdf:type owl:ObjectProperty ;
             owl:inverseOf :haVinto ;
             rdfs:domain :Gara ;
             rdfs:range :Pilota ;
             rdfs:comment "mette in relazione una gara con il pilota che ha vinto tale gara"@it ;
             rdfs:label "haVincitore"@it .


###  http://www.valbormida.it/attilio/f1.owl#haVinto
:haVinto rdf:type owl:ObjectProperty ;
         rdfs:domain :Pilota ;
         rdfs:range :Gara ;
         rdfs:comment "mette in relazione un pilota con una gara che tale pilota ha vinto"@it .


###  http://www.valbormida.it/attilio/f1.owl#haVintoPresso
:haVintoPresso rdf:type owl:ObjectProperty ;
               owl:inverseOf :haVistoVincere ;
               rdfs:domain :Pilota ;
               rdfs:range :Circuito ;
               owl:propertyChainAxiom ( :haVinto
                                        :tenutaPresso
                                      ) ;
               rdfs:comment "proprietà che lega un pilota ai circuiti sui quali ha vinto una gara"@it ;
               rdfs:label "haVintoPresso"@it .


###  http://www.valbormida.it/attilio/f1.owl#haVistoVincere
:haVistoVincere rdf:type owl:ObjectProperty ;
                rdfs:comment "proprietà che lega un circuito ai piloti che hanno vinto una gara disputata in tale circuito"@it ;
                rdfs:label "haVistoVincere"@it .


###  http://www.valbormida.it/attilio/f1.owl#ottieneGPV
:ottieneGPV rdf:type owl:ObjectProperty ;
            rdfs:domain :Pilota ;
            rdfs:range :Gara ;
            rdfs:comment "mette in relazione un pilota con una gara in cui tale pilota ha fatto segnare il giro più veloce"@it ;
            rdfs:label "ottieneGPV"@it .


###  http://www.valbormida.it/attilio/f1.owl#partecipa
:partecipa rdf:type owl:ObjectProperty ;
           rdfs:domain :Scuderia ;
           rdfs:range :Campionato ;
           rdfs:comment "mette in relazione una scuderia con un campionato a cui tale scuderia è iscritta"@it ;
           rdfs:label "partecipa"@it .


###  http://www.valbormida.it/attilio/f1.owl#precede
:precede rdf:type owl:ObjectProperty ;
         owl:inverseOf :segue ;
         rdfs:domain :Evento ;
         rdfs:range :Evento ;
         rdfs:comment "mette in relazione un evento con il successivo di quel campionato"@it ;
         rdfs:label "precede"@it .


###  http://www.valbormida.it/attilio/f1.owl#segue
:segue rdf:type owl:ObjectProperty ;
       rdfs:comment "mette in relazione un evento con il precedente di quel campionato"@it ;
       rdfs:label "segue"@it .


###  http://www.valbormida.it/attilio/f1.owl#tenutaPresso
:tenutaPresso rdf:type owl:ObjectProperty ;
              rdfs:domain :Gara ;
              rdfs:range :CircuitoOmologato ;
              rdfs:comment "mette in relazione una gara con il circuito su cui tale gara è corsa"@it ;
              rdfs:label "tenutaPresso"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.valbormida.it/attilio/f1.owl#anno
:anno rdf:type owl:DatatypeProperty ,
               owl:FunctionalProperty ;
      rdfs:domain :Campionato ;
      rdfs:range xsd:int ;
      rdfs:comment "Anno di svolgimento del campionato"@it ;
      rdfs:label "anno"@it .


###  http://www.valbormida.it/attilio/f1.owl#lunghezza
:lunghezza rdf:type owl:DatatypeProperty ;
           rdfs:domain :Circuito ;
           rdfs:range xsd:int ;
           rdfs:comment "lunghezza in metri del circuito, intesa come misurando sempre al centro del tracciato, e non in riferimento alla racing line o alla traiettoria più interna o esterna"@it ;
           rdfs:label "lunghezza"@it .


###  http://www.valbormida.it/attilio/f1.owl#numeroDiGara
:numeroDiGara rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Pilota ;
              rdfs:range xsd:int ;
              rdfs:comment "numero di gara identificativo del pilota. Viene apposto su ogni vettura che scende in pista guidata da tale pilota. Piloti ritirati o non partecipanti a un campionato vedono il proprio numero rimanergli \"riservato\" per i successivi due anni, dopodiché tornerà libero di essere scelto da qualsiasi altro pilota"@it ;
              rdfs:label "numero di gara"@it .


###  http://www.valbormida.it/attilio/f1.owl#sigla
:sigla rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       rdfs:domain :Pilota ;
       rdfs:range xsd:string ;
       rdfs:comment "abbreviazione \"ufficiale\" del pilota. Generalmente composta dalle prime tre lettere del cognome, in casi particolari può essere modificata ad hoc per evitare ambiguità"@it ;
       rdfs:label "sigla"@it .


###  http://www.valbormida.it/attilio/f1.owl#trackRecord
:trackRecord rdf:type owl:DatatypeProperty ;
             rdfs:domain :Circuito ;
             rdfs:range xsd:string ;
             rdfs:comment "con record della pista si intende il miglior tempo fatto registrare sul layout attuale del circuito. Circuiti come Albert Park, Catalogna e Yas Marina hanno subito modifiche negli ultimi anni, cambiandone le velocità e quindi anche i tempi di percorrenza"@it ;
             rdfs:label "record della pista"@it .


#################################################################
#    Classes
#################################################################

###  http://www.valbormida.it/attilio/f1.owl#Campionato
:Campionato rdf:type owl:Class ;
            rdfs:comment "Un campionato è una stagione annuale in cui viene disputata una serie di eventi e che alla fine assegna il titolo piloti e il titolo costruttori"@it ;
            rdfs:label "Campionato"@it .


###  http://www.valbormida.it/attilio/f1.owl#Circuito
:Circuito rdf:type owl:Class ;
          rdfs:comment "un circuito è una sequenza di rettilinei e curve su cui vengono disputate manifestazioni sportive motoristiche"@it ;
          rdfs:label "Circuito"@it .


###  http://www.valbormida.it/attilio/f1.owl#CircuitoOmologato
:CircuitoOmologato rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Class ;
                                         owl:oneOf ( :AlbertPark
                                                     :Algarve
                                                     :Aragon
                                                     :AutodromoEnzoDinoFerrari
                                                     :AutodromoHermanosRodriguez
                                                     :BahrainCircuit
                                                     :BakuStreetCircuit
                                                     :BarcelonaCatalunya
                                                     :BuddhCircuit
                                                     :CircuitGillesVilleneuve
                                                     :CircuitOfTheAmericas
                                                     :EstorilCircuit
                                                     :Fiorano
                                                     :FujiSpeedway
                                                     :Hockenheimring
                                                     :Hungaroring
                                                     :IgoraDrive
                                                     :Indianapolis
                                                     :Interlagos
                                                     :IstanbulPark
                                                     :JeddahCornicheCircuit
                                                     :Jerez
                                                     :KoreaCircuit
                                                     :LusailCircuit
                                                     :MarinaBay
                                                     :MiamiCircuit
                                                     :Montecarlo
                                                     :Monza
                                                     :Mugello
                                                     :NeversMagnyCours
                                                     :Nurburgring
                                                     :PaulRicard
                                                     :RedBullRing
                                                     :RicardoTormo
                                                     :Sepang
                                                     :Shanghai
                                                     :Silverstone
                                                     :Sochi
                                                     :SpaFrancorchamps
                                                     :Suzuka
                                                     :YasMarina
                                                     :Zandvoort
                                                   )
                                       ] ;
                   rdfs:subClassOf :Circuito ;
                   rdfs:comment "un circuito omologato è un circuito che ha ottenuto la massima certificazione della FIA (grade 1), necessaria per ospitare eventi di Formula 1."@it ;
                   rdfs:label "CircuitoOmologato"@it .


###  http://www.valbormida.it/attilio/f1.owl#Evento
:Evento rdf:type owl:Class ;
        rdfs:comment "un Evento è l'edizione stagionale di un certo Gran Premio"@it ;
        rdfs:label "Evento"@it .


###  http://www.valbormida.it/attilio/f1.owl#Gara
:Gara rdf:type owl:Class ;
      rdfs:comment "una gara è la manifestazione sportiva motoristica che si svolge nel corso di un evento di Formula 1"@it ;
      rdfs:label "Gara"@it .


###  http://www.valbormida.it/attilio/f1.owl#GranPremio
:GranPremio rdf:type owl:Class ;
            rdfs:comment "un Gran Premio è una manifestazione di Formula 1 che vede edizioni organizzate a cadenza annuale"@it ;
            rdfs:label "GranPremio"@it .


###  http://www.valbormida.it/attilio/f1.owl#Pilota
:Pilota rdf:type owl:Class ;
        rdfs:comment "un pilota è un atleta che corre per una scuderia iscritta a un campionato di Formula 1"@it ;
        rdfs:label "Pilota"@it .


###  http://www.valbormida.it/attilio/f1.owl#PilotaCampione
:PilotaCampione rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Pilota
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :campioneMondiale ;
                                                             owl:someValuesFrom :Campionato
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf :Pilota ;
                rdfs:comment "indica un pilota che ha vinto almeno una volta un campionato piloti"@it ;
                rdfs:label "PilotaCampione"@it .


###  http://www.valbormida.it/attilio/f1.owl#PilotaGrandeVincitore
:PilotaGrandeVincitore rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :PilotaVincitore
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :haVinto ;
                                                                    owl:minQualifiedCardinality "10"^^xsd:nonNegativeInteger ;
                                                                    owl:onClass :Gara
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf :PilotaVincitore ;
                       rdfs:comment "indica un pilota che ha vinto almeno dieci gare"@it ;
                       rdfs:label "PilotaGrandeVincitore"@it .


###  http://www.valbormida.it/attilio/f1.owl#PilotaMoltePole
:PilotaMoltePole rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( :PilotaPoleSitter
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :haPresoPole ;
                                                              owl:minQualifiedCardinality "10"^^xsd:nonNegativeInteger ;
                                                              owl:onClass :Gara
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf :PilotaPoleSitter ;
                 rdfs:comment "indica un pilota che ha ottenuto almeno dieci volte di partire dalla pole position"@it ;
                 rdfs:label "PilotaMoltePole"@it .


###  http://www.valbormida.it/attilio/f1.owl#PilotaMoltoVeloce
:PilotaMoltoVeloce rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( :PilotaVeloce
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :ottieneGPV ;
                                                                owl:minQualifiedCardinality "10"^^xsd:nonNegativeInteger ;
                                                                owl:onClass :Gara
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf :PilotaVeloce ;
                   rdfs:comment "indica un pilota che ha fatto segnare il giro più veloce in una gara almeno dieci volte"@it ;
                   rdfs:label "PilotaMoltoVeloce"@it .


###  http://www.valbormida.it/attilio/f1.owl#PilotaPoleSitter
:PilotaPoleSitter rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :Pilota
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :haPresoPole ;
                                                               owl:someValuesFrom :Gara
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf :Pilota ;
                  rdfs:comment "indica un pilota che ha ottenuto almeno una volta di partire dalla pole position in una gara"@it ;
                  rdfs:label "PilotaPoleSitter"@it .


###  http://www.valbormida.it/attilio/f1.owl#PilotaVeloce
:PilotaVeloce rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :Pilota
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :ottieneGPV ;
                                                           owl:someValuesFrom :Gara
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf :Pilota ;
              rdfs:comment "indica un pilota che ha fatto segnare almeno una volta in giro più veloce in una gara"@it ;
              rdfs:label "PilotaVeloce"@it .


###  http://www.valbormida.it/attilio/f1.owl#PilotaVincitore
:PilotaVincitore rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( :Pilota
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :haVinto ;
                                                              owl:someValuesFrom :Gara
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf :Pilota ;
                 rdfs:comment "indica un pilota che ha vinto almeno una gara"@it ;
                 rdfs:label "PilotaVincitore"@it .


###  http://www.valbormida.it/attilio/f1.owl#Scuderia
:Scuderia rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Class ;
                            owl:unionOf ( :ScuderiaCliente
                                          :ScuderiaCostruttore
                                        )
                          ] ;
          owl:disjointUnionOf ( :ScuderiaCliente
                                :ScuderiaCostruttore
                              ) ;
          rdfs:comment "una scuderia è una squadra corse iscritta a un campionato di Formula 1"@it ;
          rdfs:label "Scuderia"@it .


###  http://www.valbormida.it/attilio/f1.owl#ScuderiaCliente
:ScuderiaCliente rdf:type owl:Class ;
                 rdfs:subClassOf :Scuderia ;
                 rdfs:comment "indica una scuderia che non costruisce la propria power unit, ma che tramite accordo commerciale la riceve in fornitura da un'altra scuderia"@it ;
                 rdfs:label "ScuderiaCliente"@it .


###  http://www.valbormida.it/attilio/f1.owl#ScuderiaCostruttore
:ScuderiaCostruttore rdf:type owl:Class ;
                     rdfs:subClassOf :Scuderia ;
                     rdfs:comment "indica una scuderia che costruisce la propria power unit da montare sulle vetture. Può eventualmente fornire tale power unit anche ad altre scuderie che non costruiscono la propria"@it ;
                     rdfs:label "ScuderiaCostruttore"@it .


